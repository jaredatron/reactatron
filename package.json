{
  "name": "reactatron",
  "version": "0.0.4",
  "description": "A Personal React Framework",
  "main": "index.js",
  "engines": {
    "node": ">=6.3.0"
  },
  "bin": {
    "reactatron": "./dist/bin/reactatron.js"
  },
  "scripts": {
    "compile": "babel -d dist/ src/",
    "watch": "babel -w -d dist/ src/",
    "prepublish": "npm run compile",
    "start": "node dist/index.js",
    "test": "npm run compile && mocha"
  },
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      "transform-runtime"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/deadlyicon/reactatron.git"
  },
  "author": "Jared Grippe",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/deadlyicon/reactatron/issues"
  },
  "homepage": "https://github.com/deadlyicon/reactatron#readme",
  "dependencies": {
    "babel-runtime": "6.1.18",
    "body-parser": "^1.15.2",
    "express": "^4.14.0"
  },
  "devDependencies": {
    "babel-cli": "6.1.18",
    "babel-middleware": "^0.3.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "expect.js": "^0.3.1",
    "mocha": "2.3.4"
  },
  "peerDependencies": {
    "babel-cli": "6.1.18",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "express": "^4.14.0"
  }
}
